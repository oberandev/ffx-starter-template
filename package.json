{
  "name": "ffx-starter-template",
  "description": "FFX typescript starter template for Flatfile configuration",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "keywords": [
    "ffx",
    "flatfile"
  ],
  "author": "Oberan (https://oberan.dev)",
  "homepage": "https://github.com/oberandev/ffx-starter-template#readme",
  "bugs": {
    "url": "https://github.com/oberandev/ffx-starter-template/issues"
  },
  "private": true,
  "engines": {
    "node": ">= 18"
  },
  "module": "./src/main.ts",
  "main": "./src/main.ts",
  "scripts": {
    "build": "make build",
    "clean": "make clean",
    "compile": "make compile",
    "deploy": "make deploy",
    "format": "make format",
    "lint": "make lint",
    "lint:fix": "make lint-fix",
    "prepare": "is-ci || husky install",
    "test": "make test",
    "test:coverage": "make test-coverage",
    "test:ui": "make test-ui"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.3",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "**/*.{json,md}": [
      "prettier --write"
    ],
    "**/*.{js,mjs,mts,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@flatfile/listener": "^0.3.17"
  }
}
